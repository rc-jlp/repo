{
  "name": "ghostkit/changelog",
  "category": "text",
  "title": "Changelog",
  "description": "Show the changes log of your product.",
  "keywords": ["changelog", "log"],
  "supports": {
    "html": false,
    "className": false,
    "anchor": true,
    "align": ["wide", "full"]
  },
  "attributes": {
    "version": {
      "type": "string",
      "default": ""
    },
    "date": {
      "type": "string",
      "default": ""
    }
  }
}
