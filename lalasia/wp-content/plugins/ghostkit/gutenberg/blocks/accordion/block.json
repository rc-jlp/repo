{
  "name": "ghostkit/accordion",
  "category": "design",
  "title": "Accordion",
  "description": "Toggle the visibility of content across your project.",
  "keywords": ["accordion", "collapsible", "collapse"],
  "supports": {
    "html": false,
    "className": false,
    "anchor": true,
    "align": ["wide", "full"]
  },
  "attributes": {
    "itemsCount": {
      "type": "number",
      "default": 2
    },
    "collapseOne": {
      "type": "boolean",
      "default": false
    }
  }
}
