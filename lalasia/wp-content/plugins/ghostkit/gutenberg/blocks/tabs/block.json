{
  "name": "ghostkit/tabs-v2",
  "category": "design",
  "title": "Tabs",
  "description": "Separate content on the tabs with titles.",
  "keywords": ["tabs", "tab"],
  "supports": {
    "html": false,
    "className": false,
    "anchor": true,
    "align": ["wide", "full"]
  },
  "attributes": {
    "tabActive": {
      "type": "string",
      "default": "tab-1"
    },
    "buttonsVerticalAlign": {
      "type": "boolean",
      "default": false
    },
    "buttonsAlign": {
      "type": "string",
      "default": "start"
    },
    "tabsData": {
      "type": "array",
      "default": [
        {
          "slug": "tab-1",
          "title": "Tab 1"
        },
        {
          "slug": "tab-2",
          "title": "Tab 2"
        }
      ]
    }
  }
}
